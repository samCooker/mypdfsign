<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrollBar">
        <attr name="sb_handlerColor" format="color|reference" />
        <attr name="sb_indicatorColor" format="color|reference" />
        <attr name="sb_indicatorTextColor" format="color|reference" />
        <attr name="sb_horizontal" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="ContentView">
        <attr format="dimension|reference" name="asWidth"/>
        <attr format="color|reference" name="asTitleColor"/>
        <attr format="dimension|reference" name="asTitleTextSize"/>
        <attr format="color|reference" name="asDefaultColor"/>
        <attr format="color|reference" name="asDestructiveColor"/>
        <attr format="dimension|reference" name="asActionTextSize"/>
    </declare-styleable>
</resources>